{
  "version": 2,
  "builds": [
    { "src": "main.py", "use": "@vercel/python" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/send_order", "methods":["POST"], "dest": "/main.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
